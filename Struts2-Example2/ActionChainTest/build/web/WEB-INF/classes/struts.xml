<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="action" extends="struts-default" namespace="/action">
		<action name="actionChain1" class="action.ActionChain1">
			<result type="chain">actionChain2</result>
		</action>
		<action name="actionChain2" class="action.ActionChain2">
			<result type="chain">actionChain3</result>
		</action>
		<action name="actionChain3" class="action.ActionChain3">
			<result>/pages/actionChainResult.jsp</result>
		</action>
	</package>
</struts>